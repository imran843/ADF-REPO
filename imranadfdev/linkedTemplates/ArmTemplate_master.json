{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "imranadfdev"
		},
		"ADLS_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_LS'"
		},
		"ADLS_STORAGE_LS_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ADLS_STORAGE_LS'"
		},
		"AzureSqlDatabase_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'AzureSqlDatabase_LS'"
		},
		"Snowflake_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'Snowflake_LS'"
		},
		"linkedService_ls_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'linkedService_ls'"
		},
		"logging_LS_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'logging_LS'"
		},
		"ADLS_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://imran1993adls.dfs.core.windows.net/"
		},
		"ADLS_STORAGE_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://imranadlsstorage.dfs.core.windows.net/"
		},
		"RestService_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://reqres.in/api/users"
		},
		"SourceHttpServer_LS_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://drive.google.com"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "imranadfdev_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ADLS_LS_accountKey": {
						"value": "[parameters('ADLS_LS_accountKey')]"
					},
					"ADLS_STORAGE_LS_accountKey": {
						"value": "[parameters('ADLS_STORAGE_LS_accountKey')]"
					},
					"AzureSqlDatabase_LS_connectionString": {
						"value": "[parameters('AzureSqlDatabase_LS_connectionString')]"
					},
					"Snowflake_LS_connectionString": {
						"value": "[parameters('Snowflake_LS_connectionString')]"
					},
					"linkedService_ls_connectionString": {
						"value": "[parameters('linkedService_ls_connectionString')]"
					},
					"logging_LS_connectionString": {
						"value": "[parameters('logging_LS_connectionString')]"
					},
					"ADLS_LS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_LS_properties_typeProperties_url')]"
					},
					"ADLS_STORAGE_LS_properties_typeProperties_url": {
						"value": "[parameters('ADLS_STORAGE_LS_properties_typeProperties_url')]"
					},
					"RestService_LS_properties_typeProperties_url": {
						"value": "[parameters('RestService_LS_properties_typeProperties_url')]"
					},
					"SourceHttpServer_LS_properties_typeProperties_url": {
						"value": "[parameters('SourceHttpServer_LS_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		},
		{
			"name": "imranadfdev_ArmTemplate_1",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_1.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'imranadfdev_ArmTemplate_0')]"
			]
		},
		{
			"name": "imranadfdev_ArmTemplate_2",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_2.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'imranadfdev_ArmTemplate_1')]"
			]
		},
		{
			"name": "imranadfdev_ArmTemplate_3",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_3.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					}
				}
			},
			"dependsOn": [
				"[resourceId('Microsoft.Resources/deployments', 'imranadfdev_ArmTemplate_2')]"
			]
		}
	]
}